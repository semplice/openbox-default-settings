#mimeapps.list /etc/skel/.local/share/applications
volumeicon /etc/skel/.config
clipit /etc/skel/.config

# Lightdm
lightdm/* /usr/share/semplice-default-settings/lightdm

# Chromium
chromium/* /usr/share/semplice-default-settings/chromium

