openbox/menu.xml /etc/skel/.config/openbox
openbox/rc.xml /etc/skel/.config/openbox

# Conky stuff
conky/semplice.conf /etc/conky
conky/conky-fortune.py /usr/share/semplice-default-settings/advanced
conky/conky-harddisks.sh /usr/share/semplice-default-settings/advanced

# Panel stuff
tint2rc /etc/xdg/tint2
classic.tint2rc /etc/xdg/tint2

# fonts
fonts/ /etc

# gsimplecal
gsimplecal/ /etc/skel/.config

# Terminal
roxterm.sourceforge.net/ /etc/skel/.config

# File Manager
pcmanfm /etc/skel/.config
libfm /etc/skel/.config

# Mirage (image viewer)
mirage /etc/skel/.config

# Gconf - sigh! - settings
gconf/* /usr/share/gconf/defaults

# Xfce4 applications settings
